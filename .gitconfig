[core]
	editor = code-insiders --wait
	# pager = tig
[color]
	ui = auto
	# pager = false
[diff]
	colorMoved = plain
[push]
	default = simple
	autoSetupRemote = true
[alias]
	co = checkout
	si = switch
[pull]
	rebase = true

[commit]
  gpgsign = true
[gpg]
  format = ssh
[user]
	signingkey = ~/.ssh/id_rsa